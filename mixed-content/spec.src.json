{
  "specification": [
    {
      "name": "optionally-blockable",
      "title": "Optionally-blockable content",
      "description": "Test behavior of optionally-blockable content",
      "specification_url": "http://www.w3.org/TR/mixed-content/#category-optionally-blockable",
      "test_expansion": [
        {
          "name": "opt-in-blocks",
          "expansion": "default",
          "source_scheme": "https",
          "source_context": "*",
          "delivery_type": "*",
          "delivery_value": "opt-in",
          "redirection": "*",
          "subresource": {
            "blockable": [],
            "optionally-blockable": "*"
          },
          "origin": [
            "cross-http",
            "same-http"
          ],
          "expectation": "blocked"
        },
        {
          "name": "no-opt-in-allows",
          "expansion": "default",
          "source_scheme": "https",
          "source_context": "*",
          "delivery_type": "*",
          "delivery_value": null,
          "redirection": "*",
          "subresource": {
            "blockable": [],
            "optionally-blockable": "*"
          },
          "origin": [
            "cross-http",
            "same-http"
          ],
          "expectation": "allowed"
        }
      ]
    },
    {
